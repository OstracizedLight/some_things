a = game:GetService("RunService").Heartbeat:Connect(function()
    wait()
    for i, v in pairs(game:GetService("Players")["TheForbiddenLight_1"].PlayerGui:GetChildren()) do
        if v.Name == "CarrotGui" and v.Parent then
            v:Destroy()
            wait()
        end
